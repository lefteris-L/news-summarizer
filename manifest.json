{
  "manifest_version": 3,
  "name": "News summarizer",
  "version": "1.0.0",
  "description": "Get summarized financial news from publishers of your choice",
  "action": {
    "default_popup": "index.html"
  },
  "default_locale": "en",
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+N",
        "mac": "Command+N"
      }
    }
  }
}